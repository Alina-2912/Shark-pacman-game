    Height="635"
    ResizeMode="NoResize"
    WindowStartupLocation="CenterScreen">
    <Window.Resources>

        <!-- This is the geometry creates the animation path. Because 
     this example uses it multiple times, it's declared as a resource and
     frozen to improve performance. -->
        <PathGeometry x:Key="AnimationPath" 
        Figures="M 750,553 650,553 650,457 750,457 750,350 543,350 543,165 421,166 421,276 35,276 35,553, 162,553 162,380 296,380 296,553 536,553 296,553 296,380 162,380 162,553 35,553 35,276 421,276 421,166 543,165 543,350 750,350 750,457 650,457 650,553 750,553"
        PresentationOptions:Freeze="True" />
    </Window.Resources>
    <Canvas
        Name="MyCanvas"
        Background="Transparent"
